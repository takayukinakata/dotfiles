[user]
	name = tnakata
	email = f.seasons017@gmail.com
[alias]
	s = status
	sl = stash list
	ss = stash save
	sp = stash pop
	b = branch
	bm = branch --merged
	bd = branch -d
	a = add -A
	c = commit -m
	ch = checkout
	l = log -p --decorate
	lo = log --oneline --graph --decorate
	d = diff
	r = reset
	fpr = "!f(){ git fetch origin pull/$1/head:_check_$1; git checkout _check_$1; }; f"
[color]
	ui = true
[core]
        excludesfile = ~/.gitignore_global
        editor="emacs"
